<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout
		xmlns:android="http://schemas.android.com/apk/res/android"
		android:layout_height="match_parent"
		android:layout_width="match_parent"
		>
	
	<TextView
			android:text="@string/textFragmentOneNameFragmentNameActivityMain"
			android:id="@+id/textViewFragmentNameActivityMain"
			android:layout_height="wrap_content"
			android:layout_width="wrap_content"
			
			android:layout_alignParentStart="true"
			android:layout_alignParentEnd="true"
			android:layout_alignParentTop="true"
			
			android:layout_marginTop="100dp"
			android:gravity="center"
			android:textSize="50sp"
			android:height="120dp"
			/>
	
	<Button
			android:text="@string/textButtonSwitchTargetTwoFragmentTwoActivityMain"
			android:layout_below="@+id/textViewFragmentNameActivityMain"
			android:id="@+id/buttonSwitchFragmentFragmentOne"
			android:layout_gravity="center"
			
			android:layout_width="280dp"
			android:layout_height="280dp"
			
			android:layout_marginEnd="50dp"
			android:layout_marginTop="260dp"
			android:layout_marginStart="70dp"
			android:layout_marginBottom="120dp"
			
			android:layout_alignParentEnd="true"
			android:layout_alignParentStart="true"
			android:layout_alignParentBottom="true"
			/>

</RelativeLayout>
